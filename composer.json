{
    "name": "socialrabbit/bunny",
    "description": "A powerful Laravel package for quickly scaffolding professional websites with multiple pre-built types and features",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kisal Nelaka",
            "email": "kisalnelaka6@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "intervention/image": "^2.7",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-permission": "^5.0",
        "spatie/laravel-analytics": "^5.0",
        "spatie/laravel-sitemap": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "socialrabbit\\Bunny\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "socialrabbit\\Bunny\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "socialrabbit\\Bunny\\Providers\\BunnyServiceProvider",
                "socialrabbit\\Bunny\\Providers\\WebsiteTypeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
